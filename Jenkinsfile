pipeline {
    agent any

    stages {
        stage('Continous Download') {
            steps {
                echo 'Downloading'
            }
        }
stage('Continous Integration') {
            steps {
                echo 'Integration'
    }
}
stage('Continous Deployement') {
            steps {
                echo 'Deploying'
     }
}
stage('Continous Delivery') {
            steps {
                echo 'Delivering'
}
}
}
post {
always {
echo 'Always'
}
}
}


